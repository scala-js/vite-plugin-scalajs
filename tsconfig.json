{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2020",
    "module": "ES2020",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["es2022"],
  },
  "exclude": [
    "./dist"
  ],
}
